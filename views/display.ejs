<h1>Uploaded Data</h1>
<div>
    <% items.forEach(function(image) { %>
    <div>
        <div>
            <img src="data:image/<%=image.img.contentType%>;base64,
                 <%=image.img.data.toString('base64')%>">
            <div>
                <h5><%= image._id %></h5>
                 

<p><%= image.fname %>-<%= image.lname %></p>


            </div>
            <a href="./student_data/<%= image._id %>">Update</a>
        </div>
    </div>
    <% }) %>
</div>